{
  "name": "template",
  "version": "0.0.0",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "Simple JavaScript/NPM Copypasta for a single module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "blahblahblah": "./cli.js"
  },
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/module.template"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/module.template/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "util.rstrip": "0.0.35",
    "uuid": "^3.2.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.2",
    "@types/lodash": "^4.14.108",
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.6.6",
    "@types/uuid": "^3.4.3",
    "@types/yargs": "^11.0.0",
    "ava": "^0.25.0",
    "coveralls": "^3.0.0",
    "fs-extra": "^5.0.0",
    "nyc": "^11.7.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
    "util.fixture": "0.0.63",
    "util.pkgwrap": "^0.0.91"
  },
  "pkgwrap": {
    "cleanup": [
      "index.d.ts",
      "index.js",
      "test.d.ts",
      "test.js",
      "bin/**/*.js",
      "bin/**/*.d.ts",
      "lib/**/*.js",
      "lib/**/*.jsx",
      "lib/**/*.d.ts"
    ]
  },
  "scripts": {
    "all": "yarn install && yarn run build && yarn run docs && yarn test && yarn run report",
    "build": "yarn run clean && yarn run lint && pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "docs": "pkgwrap --docs",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "report": "pkgwrap --reporting",
    "test": "pkgwrap --testing --ava"
  }
}
