{
	"name": "template",
	"version": "0.0.1",
	"author": "jmquigley <jmquigley@outlook.com>",
	"description": "Simple JavaScript/NPM Copypasta for a single module",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jmquigley/module.template"
	},
	"bugs": {
		"url": "https://github.com/jmquigley/module.template/issues"
	},
	"engines": {
		"node": ">=7.x"
	},
	"bin": {
		"blahblahblah": "./cli.js"
	},
	"main": "./index.umd.min.js",
	"module": "./index.es.min.js",
	"types": "./index.d.ts",
	"files": [
		"cli.js",
		"index.js",
		"index.*.min.js",
		"index.d.ts"
	],
	"dependencies": {
		"lodash": "^4.17.15",
		"util.rstrip": "0.0.53",
		"uuid": "^3.3.2",
		"yargs": "^13.3.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.5.5",
		"@babel/core": "^7.5.5",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-proposal-decorators": "^7.4.4",
		"@babel/plugin-transform-modules-commonjs": "^7.5.0",
		"@babel/plugin-transform-runtime": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/runtime": "^7.5.5",
		"@types/fs-extra": "8.0.0",
		"@types/jest": "^24.0.15",
		"@types/lodash": "^4.14.136",
		"@types/node": "^12.6.8",
		"@types/power-assert": "^1.5.0",
		"@types/uuid": "^3.4.5",
		"@types/yargs": "^13.0.0",
		"babel-loader": "^8.0.6",
		"babel-minify": "^0.5.0",
		"babel-plugin-espower": "^3.0.1",
		"babel-preset-minify": "^0.5.0",
		"coveralls": "^3.0.5",
		"fs-extra": "^8.1.0",
		"jest": "^24.8.0",
		"jest-environment-jsdom-fourteen": "^0.1.0",
		"power-assert": "^1.6.1",
		"prettier": "^1.18.2",
		"tslint": "^5.18.0",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.5.3",
		"util.fixture": "0.0.72",
		"util.pkgwrap": "^0.0.112",
		"webpack": "^4.37.0",
		"webpack-cli": "^3.3.6"
	},
	"pkgwrap": {
		"cleanup": [
			"docs",
			"tsconfig.tsbuildinfo"
		]
	},
	"scripts": {
		"all": "yarn install && NODE_ENV=production yarn build && yarn docs && yarn test && yarn manifest",
		"build": "yarn lint && yarn prettier && yarn build:umd && yarn build:es && yarn build:cjs",
		"build:cjs": "babel index.ts --sourceType module --out-file index.cjs.min.js",
		"build:es": "minify index.js --sourceType module --out-file index.es.min.js",
		"build:umd": "pkgwrap --build --webpack",
		"build:cli": "babel cli.js --out-file cli.js",
		"clean": "pkgwrap --clean",
		"clean:dist": "yarn clean && rimraf node_modules/ yarn.lock",
		"coverage": "pkgwrap --coverage --jest",
		"docs": "pkgwrap --docs",
		"lint": "pkgwrap --lint",
		"manifest": "npm pack --dry-run",
		"prettier": "prettier --write \"**/*.{ts,tsx,js,json,yaml,yml,css}\"",
		"test": "jest"
	}
}
