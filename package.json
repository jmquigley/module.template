{
  "name": "template",
  "version": "0.0.0",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "Simple JavaScript/NPM Copypasta for a single module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "blahblahblah": "./cli.js"
  },
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/module.template"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/module.template/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "util.rstrip": "0.0.40",
    "uuid": "^3.3.2",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/runtime": "^7.2.0",
    "@types/fs-extra": "5.0.4",
    "@types/jest": "^23.3.12",
    "@types/lodash": "^4.14.119",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/uuid": "^3.4.4",
    "@types/yargs": "^12.0.5",
    "babel-jest": "^23.6.0",
    "coveralls": "^3.0.2",
    "fs-extra": "^7.0.1",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "util.fixture": "0.0.65",
    "util.pkgwrap": "^0.0.94"
  },
  "pkgwrap": {
    "cleanup": [
      "index.d.ts",
      "index.js",
      "cli.d.ts",
      "cli.js"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "notify": false,
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "__tests__/helpers"
    ],
    "verbose": false
  },
  "scripts": {
    "all": "yarn install && yarn run build && yarn run docs && yarn test",
    "build": "yarn run clean && yarn run lint && pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage --jest",
    "docs": "pkgwrap --docs",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "report": "pkgwrap --reporting",
    "test": "pkgwrap --testing --jest"
  }
}
