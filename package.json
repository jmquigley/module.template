{
	"name": "template",
	"version": "0.0.0",
	"author": "jmquigley <jmquigley@outlook.com>",
	"description": "Simple JavaScript/NPM Copypasta for a single module",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jmquigley/module.template"
	},
	"bugs": {
		"url": "https://github.com/jmquigley/module.template/issues"
	},
	"engines": {
		"node": ">=7.4"
	},
	"dependencies": {
		"lodash": "^4.17.4",
		"object-assign": "^4.1.1",
		"fs-extra": "^2.0.0",
		"yargs": "^6.5.0"
	},
	"devDependencies": {
		"ava": "^0.17.0",
		"coveralls": "^2.11.15",
		"nyc": "^10.1.2",
		"xo": "^0.17.1"
	},
	"main": "index.js",
	"bin": {
		"blahblahblah": "./cli.js"
	},
	"scripts": {
		"test": "xo && nyc ava --verbose",
		"report": "./node_modules/.bin/nyc report --reporter=html"
	},
	"xo": {
		"env": {
			"browser": true,
			"node": true,
			"es6": true
		},
		"global": [
			"window",
			"describe",
			"it",
			"JSON"
		],
		"rules": {
			"space-before-function-paren": "off",
			"unicorn/filename-case": [
				"error",
				{
					"case": "snakeCase"
				}
			],
			"arrow-parens": [
				"error",
				"as-needed"
			]
		}
	}
}
